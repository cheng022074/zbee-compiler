{
    "command":{
        "version":"version",
        "properties":"properties",
        "compile":"compile",
        "package":"package",
        "code":"code",
        "test":"test",
        "watch":"watch",
        "web":"web",
        "backup":"backup"
    },
    "scope":{
        "folders":{
            "src":"src",
            "config":"config",
            "command":"command",
            "html":"html",
            "test":"test",
            "template":"template",
            "ui":"ui",
            "ue":"ue"
        },
        "suffixes":{
            "ui":[
                ".json"
            ],
            "ue":[
                ".js"
            ],
            "command":[
                ".js"
            ],
            "src":[
                ".fn.js",
                ".class.js"
            ],
            "config":[
                ".json"
            ],
            "template":[
                ".js",
                ".html"
            ],
            "test":[
                ".js",
                ".json"
            ]
        },
        "bin":{
            "command":{
                ".js":"bin"   
            },
            "src":{
                ".fn.js":"bin",
                ".class.js":"bin"
            },
            "config":{
                ".json":"json"
            },
            "template":{
                ".js":"text",
                ".html":"text",
                ".json":"text"
            },
            "test":{
                ".js":"bin",
                ".json":"json"
            }
        },
        "default":"src"
    },

    "path":{
        "dist":"dist",
        "bin":"bin",
        "lib":"lib",
        "backup":"backup"
    },

    "compile":{
        "scope":{
            "command":{
                ".js":{
                    "from":"script.function",
                    "to":"script"
                }
            },
            "src":{
                ".fn.js":{
                    "from":"script.function",
                    "to":"script"
                },
                ".class.js":{
                    "from":"script.class",
                    "to":"script"
                }
            },
            "test":{
                ".js":{
                    "from":"script.function.test",
                    "to":"script"
                }
            }
        },
        "batch":[]
    },
    "package":{
        "default":{
            "includes":"*",
            "scope":{
                "src":{
                    ".fn.js":"script.function",
                    ".class.js":"script.class"
                }
            },
            "to":"script"
        }
    },
    "backup":{
        "default":{
            "includes":[
                "command",
                "config",
                "lib",
                "src",
                "test",
                "template",
                "ue",
                "ui",
                ".gitignore",
                "properties.json"
            ],
            "to":"zip"   
        }
    },
    "watch":[
        "command"
    ],
    "web":{
        "routers":{
            "/":"."
        },
        "port":3000
    }
}