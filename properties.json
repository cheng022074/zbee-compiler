{
    "command":{
        "version":"command::version",
        "properties":"command::properties",
        "config":"command::config",
        "dependencies":"command::dependencies",
        "compile":"command::compile",
        "run":"command::run",
        "web":"command::web",
        "vue-reinit":"command::vue.reinit",
        "vue-init":"command::vue.init",
        "vue-dev":"command::vue.dev",
        "vue-build":"command::vue.build"
    },
    "scope":{
        "folders":{
            "src":"src",
            "config":"config",
            "command":"command",
            "html":"html"
        },
        "suffixes":{
            "command":[
                ".js"
            ],
            "src":[
                ".scope.js"
            ],
            "config":[
                ".json"
            ],
            "template":[
                ".js",
                ".html"
            ],
            "html":[
                ".html.json",
                ".html"
            ]
        },
        "default":"src"
    },

    "path":{
        "dist":"dist",
        "bin":"bin"
    },

    "compile":{
        "src":{
            ".scope.js":{
                "data":"compile.template.data.script",
                "template":"compile.script.scope",
                "to":"compile.to.script"
            }
        },
        "html":{
            ".html":{
                "data":"compile.template.data.html",
                "template":"compile.html",
                "to":"compile.to.html"
            },
            ".html.json":{
                "data":"compile.template.data.html.data",
                "template":"compile.html",
                "to":"compile.to.html"
            }
        }
    },
    "web":{
        "routers":{
            "/":"."
        },
        "port":3000
    }
}