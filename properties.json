{
    "command":{
        "version":"command::version",
        "properties":"command::properties",
        "config":"command::config",
        "dependencies":"command::dependencies",
        "compile":"command::compile",
        "package":"command::package",
        "test":"command::test",
        "run":"command::run",
        "watch":"command::watch",
        "web":"command::web",
        "init":"command::project.init",
        "dev":"command::project.dev",
        "build":"command::project.build"
    },
    "scope":{
        "folders":{
            "src":"src",
            "config":"config",
            "command":"command",
            "html":"html"
        },
        "suffixes":{
            "command":[
                ".js"
            ],
            "src":[
                ".fn.js"
            ],
            "config":[
                ".json"
            ],
            "template":[
                ".js",
                ".html"
            ],
            "html":[
                ".html.json",
                ".html"
            ]
        },
        "default":"src"
    },

    "path":{
        "dist":"dist",
        "bin":"bin",
        "library":"library"
    },

    "compile":{
        "src":{
            ".fn.js":{
                "from":"code.compile.from.script.function",
                "to":"code.compile.to.script"
            }
        }
    },
    "package":{
        "default":{
            "includes":"*",
            "scope":{
                "src":{
                    ".fn.js":"code.package.from.script.function"
                }
            },
            "to":"code.package.to.script"
        }
    },
    "web":{
        "routers":{
            "/":"."
        },
        "port":3000
    }
}