<%
    const
    {
        keys
    } = Object;

%>
<?xml?>
<classes>
<%

    let names = keys(data) ; 

    for(let name of names){

        let {
            imports,
            motify,
            signature,
            code,
            entryTypes
        } = data[name] ;
%>
    <class motify="<%- motify %>" signature="<%- signature %>"<%if(entryTypes.length){%> entry-type="<%- entryTypes.join(' ')%>"<%}%>  name="<%- name %>"<%if(imports.length){%> imports="<%- imports.join(' ')%>"<%}%>>
        <![CDATA[<%- code %>]]>
    </class>
<%
    }
%>

</classes>