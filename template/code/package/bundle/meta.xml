<%
    const
    {
        keys
    } = Object;

%>
<?xml?>
<classes>
<%

    let names = keys(data) ; 

    for(let name of names){

        let {
            imports,
            motify,
            signature,
            code,
            entryTypes,
            dependentModules,
            standard
        } = data[name] ;
%>
    <class standard="<%- standard %>" motify="<%- motify %>" signature="<%- signature %>"<%if(entryTypes){%> entry-type="<%- entryTypes%>"<%}%>  name="<%- name %>"<%if(imports){%> imports="<%- imports%>"<%}%><%if(dependentModules){%> dependent-modules="<%- dependentModules%>"<%}%>>
        <![CDATA[<%- code %>]]>
    </class>
<%
    }
%>

</classes>