<%
    const
    {
        keys
    } = Object;

%>
<?xml?>
<classes>
<%

    let names = keys(data) ; 

    for(let name of names){

        let {
            imports,
            code
        } = data[name] ;
%>
    <class name="<%- name %>"<%if(imports.length){%> imports="<%- imports.join(' ')%>"<%}%>>
        <![CDATA[<%- code %>]]>
    </class>
<%
    }
%>

</classes>