<%
    const
    {
        keys
    } = Object;
%>
<?xml?>
<classes>
<%

    let names = keys(data) ; 

    for(let name of names){

        let {
            aliases,
            imports,
            code
        } = data[name] ;
%>
    <class name="<%- name %>"<%if(aliases.length){%> aliases="<%- aliases.join(' ')%>"<%}%><%if(imports.length){%> imports="<%- imports.join(' ')%>"<%}%>>
        <![CDATA[<%- code %>]]>
    </class>
<%
    }
%>

</classes>