<%
    const {
        name,
        dependencies = {},
        version,
        type,
        module,
        files,
        engines,
        author,
        license
    } = data ;
%>
{
    "name": "<%- name %>",
    "version":"<%- version %>",
    <%if(engines){%>
    "engines":<%- JSON.stringify(engines , null , 2) %>,
    <%}%>
    <%if(author){%>
    "author": "<%- author %>",
    <%}%>
    <%if(license){%>
    "license": "<%- license %>",
    <%}%>
    "files": <%- JSON.stringify(files , null , 2) %>,
    "dependencies": <%- JSON.stringify(dependencies , null , 2) %>
}