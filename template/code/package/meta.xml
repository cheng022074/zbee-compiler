<%
    const
    {
        keys
    } = Object;
%>
<?xml?>
<classes>
<%

    let names = keys(data) ; 

    for(let name of names){

        let {
            aliases,
            code
        } = data[name] ;
%>
    <class name="<%- name %>" <%if(aliases.length){%>aliases="aliases.join(' ')"<%}%>>
        <![CDATA[<%- code %>]]>
    </class>
<%
    }
%>

</classes>